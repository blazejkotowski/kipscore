<% i = 1 %>
<% for round in @rounds: %>
  <table class="round table table-striped">
    <thead>
      <tr>
        <td>Round <%= i++ %></td>
      </tr>
    </thead>
    <tbody>
    <% for match in round.models: %>
      <% mv = new Kipscore.Views.RoundRobinMatch({ model: match }) %>
      <!--<tr class="round-robin-match">
      </tr>-->
      <% $("tbody").last().append(mv.render().$el) %>
    <% end %>
    </tbody>
  </table>
<% end %>
